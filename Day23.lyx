\end_inset


\end_layout

\begin_layout Standard
results in:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W(d,\bar{d},d^{*})=\frac{{\left(1\text{−}c\right)\, d}}{{1\text{−}c\, d}}+\frac{{1\text{−}d}}{{\left(1\text{−}c\right)\, d+d+1}}
\]

\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
\frac{\partial w(d,\bar{d},d^{*})}{\partial d}=\frac{{1\text{−}c}}{{1\text{−}c\,{\it d\_star}}}\text{−}\frac{{1}}{{\left(1\text{−}c\right)\,{\it d\_star}\text{−}{\it d\_bar}+1}}
\]

\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
\frac{\partial w(d,\bar{d},d^{*})}{\partial\bar{d}}=\text{−}\frac{{1\text{−}d}}{{\left(\left(1\text{−}c\right)\,{\it d\_star}+{\it d\_bar}+1\right)^{2}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{dw}{dd}=\left(r\frac{\partial w}{\partial d}+R\frac{\partial w}{\partial\bar{d}}\right)_{d=\bar{d}=d^{*}}=\left(\frac{{1\text{−}c}}{{1\text{−}c\, d}}\text{−}\frac{{1}}{{\left(1\text{−}c\right)\, d-\bar{d}+1}}\right)\, r\text{−}\frac{{\left(1\text{−}d\right)\, R}}{{\left(\left(1\text{−}c\right)\, d-\bar{d}+1\right)^{2}}}
\]

\end_inset


\end_layout

\begin_layout Standard
When setting 
\begin_inset Formula $r=1$
\end_inset

 (mother is totally related to her offspring, thanks to clonal reproduction),
 solve 
\begin_inset Formula $\frac{dw}{dd}=0$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{dw}{dd}=\left(\frac{{1\text{−}c}}{{1\text{−}c\, d}}\text{−}\frac{{1}}{{\left(1\text{−}c\right)\, d-\bar{d}+1}}\right)\, r\text{−}\frac{{\left(1\text{−}d\right)\, R}}{{\left(\left(1\text{−}c\right)\, d-\bar{d}+1\right)^{2}}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(\frac{{1\text{−}c}}{{1\text{−}c\, d}}\text{−}\frac{{1}}{{\left(1\text{−}c\right)\, d-\bar{d}+1}}\right)\,−\frac{{\left(1\text{−}d\right)\, R}}{{\left(\left(1\text{−}c\right)\, d-\bar{d}+1\right)^{2}}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
d=\frac{{R\text{−}c}}{{R-c^{2}}}
\]

\end_inset


\end_layout

